pipeline {
  agent { label 'slave1' }
tools {
        jdk "Java_home" // the name you have given the JDK installation in Global Tool Configuration
    }
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'server-sonar') { 
          sh './mvnw clean org.sonarsource.scanner.maven:sonar-maven-plugin:3.9.1.2184:sonar'
        }
      }
    }
  }
}
